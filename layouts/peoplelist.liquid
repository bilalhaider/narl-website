<section>
    <div class="container">
        <div class="row">

            <div class="col-sm-12">Principal Investigator</div>
            {% for people in collections.PrincipalInvestigator %}
            <div class="col-sm-12">
                <div class="description">
                    <a href="{{people.url}}">
                        <img src="{{people.url | url}}{{people.data.profile.picture}}">
                        {{people.data.profile.name}}
                        {{people.data.profile.designation}}
                    </a>
                </div>
            </div>
            {% endfor %}

