    <section class="white-section">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <div class="sec-title">
                        <!-- <p>What We Do Now</p> -->
                        <h2>Stats</h2>
                        <div class="separator"></div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-3">
                    <div class="stat">
                        <span class="number"> 1000</span>
