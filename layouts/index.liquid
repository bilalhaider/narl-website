    </section>

    <section class="gray-section">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <div class="sec-title">
                        <h2>Research Themes</h2>
                        <div class="separator"></div>
                    </div>
                </div>
            </div>

            <div class="row">
                {% for area in collections.researchareas %}
                <div class="col-sm-4">
                    <div class="card">
                        <img src="{{ area.url | url }}/{{ area.data.image }}" class="card-img-top card-img" alt=" ...">
                        <div class="card-body">
                            <h5 class="card-title">{{ area.data.title }}</h5>
                            <p class="card-text">{% shortened area.templateContent %}</p>
                        </div>
                    </div>
                </div>
                {% endfor %}
            </div>

        </div>
    </section>

