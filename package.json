{
  "name": "narl-website",
  "version": "1.0.0",
  "description": "Public website for NARL Lab at LUMS",
  "main": "index.html",
  "scripts": {
    "sass": "sass --style=compressed src/scss/styles.scss generated/css/styles.css",
    "watch:eleventy": "eleventy --serve",
    "watch:sass": "npm run sass -- --watch",
    "watch": "npm-run-all sass --parallel watch:*",
    "build": "npm run sass && eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bilalhaider/narl-website.git"
  },
  "author": "Bilal Haider",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bilalhaider/narl-website/issues"
  },
  "homepage": "https://github.com/bilalhaider/narl-website#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.26.10"
  }
}